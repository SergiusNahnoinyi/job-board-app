{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "next",
    "next/core-web-vitals",
    "prettier",
    "eslint:recommended",
    "eslint-config-next",
    "eslint-config-prettier",
    "plugin:eslint-plugin-tailwindcss/recommended"
  ],
  "rules": {
    "prettier/prettier": "error",
    "prefer-const": "error",
    "no-unused-vars": "warn"
  },
  "plugins": ["eslint-plugin-prettier", "eslint-plugin-tailwindcss"]
}
