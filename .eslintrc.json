{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["next", "next/core-web-vitals", "prettier"],
  "rules": {
    "prefer-const": "error",
    "no-unused-vars": "warn",
    "no-console": "off",
    "react-hooks/exhaustive-deps": "off"
  },
  "plugins": ["eslint-plugin-tailwindcss"]
}
